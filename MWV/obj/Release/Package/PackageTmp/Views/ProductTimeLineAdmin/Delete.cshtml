@model MWV.Models.ProductionTimeline
@{
    ViewBag.Title = "Delete";
}

<h2><b> Delete Product ->@Html.ActionLink("Goto Home", "Index")</b></h2>
<br /><br />
  &nbsp;<span id="ErrorMsgs"></span>
<br />
&nbsp;<h3 class="form-label">&nbsp; Are you sure you want to delete this ProductionTimeline ?</h3>
<div>
    </br>
    <input type="hidden" value="@Model.production_timeline_id" id="hdnProductTLIDToDel" />
    <input type="hidden" value="@Model.papermill_id" id="hdnPaperMillID" />
    <div class="form-group">
        <p class="form-label">&nbsp;PaperMill</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.PaperMill.name, new { @class = "text-input", id = "PaperMill", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <p class="form-label">&nbsp;BFCode</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.bf_code, new { @class = "text-input", id = "BFCode", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <p class="form-label">&nbsp;GSMCode</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.gsm_code, new { @class = "text-input", id = "GSMCode", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <p class="form-label">&nbsp;Shade Code</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.shade_code, new { @class = "text-input", id = "ShadeCode", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
 
        @Html.AntiForgeryToken()
    </br> </br>
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" id="btnDeleteProductTL" />
        </div>
    <br />
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="button" class="btn btn-default" title="Cancel" value="Cancel" id="btnCancel" onclick="location.href='@Url.Action("Index", "ProductTimeLineAdmin")'" />
        </div>
    </div><br />
  
   
</div>
    