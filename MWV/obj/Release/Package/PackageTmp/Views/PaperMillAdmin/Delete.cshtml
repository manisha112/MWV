@model MWV.Models.Papermill

@{
    ViewBag.Title = "Delete";
}

<h2><b> Delete Papermill ->@Html.ActionLink("Goto Home", "Index")</b></h2>
<br /><br />
  &nbsp;<span id="ErrorMsgs"></span>
<br />
&nbsp;<h3 class="form-label">&nbsp; Are you sure you want to delete this PaperMill ?</h3>
<div>
    <input type="hidden" value="@Model.papermill_id" id="PaperMillIDToDel" />

    <div class="form-group">
        <p class="form-label">&nbsp;Location</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.location, new { @class = "text-input", id = "location", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <p class="form-label">&nbsp;Name</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.name, new { @class = "text-input", id = "name", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    <div class="form-group">
        <p class="form-label">&nbsp;Address</p>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.address, new { @class = "text-input", id = "address", @readonly = "readonly", disabled = "disabled" })
        </div>
    </div>
    </br>

    @Html.AntiForgeryToken()
    <div class="form-actions no-color">
        <input type="submit" value="Delete" class="btn btn-default" id="btnDeletePaperMill" />
    </div>
    <br />
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="button" class="btn btn-default" title="Cancel" value="Cancel" id="btnCancel" onclick="location.href='@Url.Action("Index", "PaperMillAdmin")'" />
        </div>
    </div>
    <br />
    <br />
</div>
</br>