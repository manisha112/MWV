@model IEnumerable<MWV.Repository.Implementation.OrderRepository.tempOrderProducts>

<div class="review_order_details">
    @foreach (var item in Model)
    {
        <p class="customer_name"><strong>Customer Name</strong></p>
        <p class="delivery_date">Requested Delivery Date: @ViewBag.order_del_date</p>
        <ul>

            <li>
                <p class="product_code"><strong>@item.product_code</strong></p>
                <p class="product_details">width: @item.width , qty: @item.qty</p>
                <p class="product_details">unit price: INR @item.unit_price</p>
            </li>
            @*<li>
                    <p class="product_code"><strong>TK-144.0-185-40RGMP</strong></p>
                    <p class="product_details">144.0 x 185 BF 40 RUBY KRAFT TOP</p>
                    <p class="product_details">INR 29,335</p>
                </li>*@
        </ul>
    }
    <div class="review_order_totals">
        @*<p><strong>Total: INR 29,335</strong></p>*@
        <p><strong>Grand Total: @ViewBag.grandtotal</strong></p>
    </div>
</div>