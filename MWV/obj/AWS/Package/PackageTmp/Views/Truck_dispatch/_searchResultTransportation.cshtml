@model IEnumerable<MWV.Models.Truck_dispatches>

    <h4><strong>TRUCK DISPATCH VIEW </strong> </h4>
    <div class="fix-overflow">
        <a href="#closeTransportationResultsPanel" class="close-panel"><img src="images/close-panel.png"></a>
        Sort By + -
    </div>
    <table class="table" cellspacing="5" cellpadding="2" style="padding:10px">
        <div style="padding:10px">
            @ViewBag.noTruckCroMsg
        </div>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    <strong> Truck No </strong>  : @item.truck_no
                </td>
            </tr>
            <tr>
                <td>
                    Estimated Arrival :   @item.estimated_arrival
                </td>
            </tr>
            <tr>
                <td>
                    Estimated Capacity :  @item.estimated_capacity
                </td>
            </tr>
            <tr>
                <td>
                    Agent_Dispatched On :   @item.agent_dispatched_on
                </td>
            </tr>
            <tr>
                <td>
                    Status :  @item.status
                </td>
            </tr>
            <tr></tr>
        }

    </table>
