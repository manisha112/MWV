

    <div id="PaperMillList">
        @Html.DropDownList("viewPaperMillList", (SelectList)ViewBag.lstPapermills, "Select Papermill", new { @class = "select-input" })
    </div>
    <div id="viewSchedule">

    </div>

<script type="text/javascript">
    $('#PaperMillList').find('#viewPaperMillList').on('change', function () {
        debugger;
        var selectedItem = $(this).val();

        $.ajax(
                   {
                       cache: false,
                       type: "GET",
                       url: "/Agent/ScheduleDetails/" + selectedItem,
                       contentType: "application/json; charset=utf-8",
                       success: function (data) {
                           $('#viewSchedule').html(data);
                       },
                       error: function (xhr, ajaxOptions, thrownError) {

                       }
                   });
    });
</script>
